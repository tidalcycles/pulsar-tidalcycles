{
  "name": "tidalcycles",
  "main": "./lib/tidalcycles",
  "version": "4.1.3",
  "description": "TidalCycles, a language for live coding algorithmic patterns.",
  "atomTestRunner": "runners/jasmine2-test-runner",
  "activationCommands": {
    "atom-text-editor": [
      "tidalcycles:eval",
      "tidalcycles:eval-multi-line"
    ],
    "atom-workspace": [
      "tidalcycles:boot",
      "tidalcycles:reboot"
    ]
  },
  "dependencies": {
    "directory-tree": "^3.5.2",
    "osc-min": "^2.1.2"
  },
  "repository": "git@github.com:tidalcycles/pulsar-tidalcycles.git",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provide"
      }
    }
  }
}
