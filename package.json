    "osc-min": "^1.1.2"
  },
  "repository": "git@github.com:tidalcycles/pulsar-tidalcycles.git",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "autocompleteProvider"
      }
    }
  }
